<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/whiteapricot"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/transparent"/>
            <stroke android:width="0.5dp" android:color="#dbdbdb"/>
            <corners android:topRightRadius="5dp" android:bottomLeftRadius="5dp"    
                     android:topLeftRadius="5dp"  android:bottomRightRadius="5dp"/>
        </shape>
    </item>
</selector>
